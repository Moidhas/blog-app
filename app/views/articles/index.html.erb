<h1>Hello Rails</h1>
<ul>
  <% @articles.each do |article|%>
    <li>
      <%= link_to article.title, article %>
      <%= link_to "Delete", article, data: {
        turbo_method: :delete,
        turbo_confirm: "Are you sure?"
      } %>
    </li>
  <%end%>
</ul>
<%= link_to "Create New Article", new_article_path%>

